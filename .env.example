# Django Settings
DJANGO_SECRET_KEY=your-secret-key-change-in-production
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database
POSTGRES_DB=zillow_scraper
POSTGRES_USER=zillow_user
POSTGRES_PASSWORD=zillow_password
DATABASE_URL=postgres://zillow_user:zillow_password@db:5432/zillow_scraper

# Redis & Celery
REDIS_URL=redis://redis:6379/0
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0

# Proxy Configuration (comma-separated list)
# Format: http://user:pass@host:port or http://host:port
PROXIES=

# User Agents (comma-separated list, optional - uses fake-useragent if empty)
USER_AGENTS=

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=500

# Scraper Settings
REQUEST_DELAY_MIN=1.0
REQUEST_DELAY_MAX=3.0
REQUEST_TIMEOUT=30
MAX_RETRIES=3
